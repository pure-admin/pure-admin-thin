    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tippy": "^6.4.1",
    "vue-types": "^5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/types": "^19.0.3",
    "@eslint/js": "^8.57.0",
    "@faker-js/faker": "^8.4.1",
    "@iconify-icons/ep": "^1.2.12",
    "@iconify-icons/ri": "^1.2.10",
    "@iconify/vue": "^4.1.1",
    "@pureadmin/theme": "^3.2.0",
    "@types/gradient-string": "^1.1.5",
